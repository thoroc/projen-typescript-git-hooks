{
  "bracketSpacing": true,
  "printWidth": 110,
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": [
        "*.md"
      ],
      "options": {
        "printWidth": 120,
        "proseWrap": "always",
        "quoteProps": "as-needed",
        "semi": true,
        "singleQuote": false
      }
    }
  ]
}
